<%
	String a = mMgr.listRead();
	JSONParser jsonParser = new JSONParser();
	JSONObject jsonObject = (JSONObject)jsonParser.parse(a);
	JSONArray dataArray = (JSONArray)jsonObject.get("imgs");
	for(int i=0; i<dataArray.size();i++){
		JSONObject tmp = (JSONObject)dataArray.get(i);
		//out.println(tmp.get("img"));
		String data = tmp.get("img")+"";
		
	%>
<script>
	var bt =  document.getElementsByClassName('wrap')[0];
    var div1 =  document.createElement('div');
    div1.className +="cont";
     var img = document.createElement('img')
     img.setAttribute('src','<%=data%>');
     img.className +="a1";
     div1.appendChild(img);
     
     bt.appendChild(div1);
	
	</script>
